{
  "name": "clickndebrid",
  "version": "1.1.0",
  "description": "A Click'n'Load proxy server that converts links via debrid services API and submits them to PyLoad et al.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ts-node scripts/copy-public.ts",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "debug": "node --inspect=${DEBUG_PORT:-0.0.0.0:9229} --require ts-node/register src/index.ts",
    "debug-brk": "node --inspect-brk=${DEBUG_PORT:-0.0.0.0:9229} --require ts-node/register src/index.ts",
    "debug:env": "node -r dotenv/config --inspect=${DEBUG_PORT:-0.0.0.0:9229} --require ts-node/register src/index.ts",
    "debug-brk:env": "node -r dotenv/config --inspect-brk=${DEBUG_PORT:-0.0.0.0:9229} --require ts-node/register src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts --max-warnings=100",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:auto": "bash ./scripts/release.sh",
    "test:release": "bash ./scripts/test-release.sh",
    "setup:husky": "bash ./scripts/setup-husky.sh",
    "prepare": "husky"
  },
  "author": "XaverLeet",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/react": "^2.2.0",
    "@heroicons/vue": "^2.2.0",
    "autoprefixer": "^10.4.20",
    "body-parser": "^1.20.2",
    "chalk": "^5.4.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "postcss": "^8.5.3",
    "redis": "^4.7.0",
    "tailwindcss": "^4.0.9",
    "vue": "^3.5.13",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/body-parser": "^1.19.5",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.5",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
