{
  "name": "clickndebrid",
  "version": "1.1.35",
  "description": "A Click'n'Load proxy server that converts links via debrid services API and submits them to PyLoad et al.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsx scripts/copy-public.ts",
    "build:skip-tests": "tsc --skipLibCheck && tsx scripts/copy-public.ts",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/index.ts",
    "debug": "tsx --inspect=${DEBUG_PORT:-0.0.0.0:9229} src/index.ts",
    "debug-brk": "tsx --inspect-brk=${DEBUG_PORT:-0.0.0.0:9229} src/index.ts",
    "debug:env": "tsx --inspect=${DEBUG_PORT:-0.0.0.0:9229} --env-file=.env src/index.ts",
    "debug-brk:env": "tsx --inspect-brk=${DEBUG_PORT:-0.0.0.0:9229} --env-file=.env src/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint . --ext .ts --max-warnings=100",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "NODE_OPTIONS=--no-deprecation release-it",
    "release:minor": "NODE_OPTIONS=--no-deprecation release-it minor",
    "release:major": "NODE_OPTIONS=--no-deprecation release-it major",
    "release:patch": "NODE_OPTIONS=--no-deprecation release-it patch",
    "release:auto": "./scripts/release.sh auto",
    "release:dry-run": "./scripts/release.sh --dry-run",
    "commit:all": "git add . && git commit",
    "push": "./scripts/git-push.sh",
    "prepare": "husky"
  },
  "author": "XaverLeet",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "chalk": "^5.4.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "postcss": "^8.4.35",
    "redis": "^4.7.0",
    "tailwindcss": "^3.4.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
